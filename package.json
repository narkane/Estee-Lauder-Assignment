    "gulp-sourcemaps": "2.6.1",
    "nodemon": "^2.0.12",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "uglify-js": "3.2.2",
    "webpack": "^5.45.1",
    "webpack-stream": "^6.1.2"
  }
